<Application x:Class="ABC.SHP.View.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         
    	<ResourceDictionary>
    		<ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resource\windows.xaml"/>
                <ResourceDictionary Source="Resource\StatusBar.xaml"/>
                <ResourceDictionary Source="Resource\Tab.xaml"/>
                <ResourceDictionary Source="Resource\Tabitem.xaml"/>
                <ResourceDictionary Source="Resource\Button.xaml"/>
                <ResourceDictionary Source="Resource\menu.xaml"/>
                <ResourceDictionary Source="Resource\list.xaml"/>
    		</ResourceDictionary.MergedDictionaries>
    	</ResourceDictionary>
         
    </Application.Resources>
</Application>
